package fundamentos;

public class DesafioAritmeticas {

	public static void main(String[] args) {
		
		int a = 3+2;
		int b = 6 * a;
		int c = (int) Math.pow(b, 2);
		int d = 3*2;
		int e = c/d;
		System.out.println(e);
		int f = 1-5;
		int g = 2-7;
		int h = f * g;
		int i = h / 2;
	//	int i = (int) Math.pow(h, 2);

	//	int k = (int) Math.pow(j,3);
		System.out.println(i);

	}

}
