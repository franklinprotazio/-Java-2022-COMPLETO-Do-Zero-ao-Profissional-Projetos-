package fundamentos;

import java.util.Scanner;

public class Temperatura {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner teclado = new Scanner(System.in);
		int f = teclado.nextInt();
		int c = (f - 32 ) * 5/9;
		System.err.println(c);
		teclado.close();

	}

}
